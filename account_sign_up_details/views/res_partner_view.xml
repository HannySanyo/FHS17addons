<!--This file is no longer being used since we added the field via studio-->
<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="res_partner_form_attachment" model="ir.ui.view">
            <field name="name">res.partner.form.attachment</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='x_studio_field_name']" position="inside">
                        <field name="x_studio_contractor_doc" filename="x_studio_contractor_doc_filename"/>
                        <field name="x_studio_contractor_doc_filename" invisible="1"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>